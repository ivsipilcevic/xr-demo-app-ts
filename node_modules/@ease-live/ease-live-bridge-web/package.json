{
  "name": "@ease-live/ease-live-bridge-web",
  "version": "1.15.2",
  "description": "Web based Ease Live player",
  "main": "es/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "build",
    "es",
    "src/plugins"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel src --out-dir es --copy-files",
    "build:examples": "cross-env BUILD_EXAMPLES=1 npm run build",
    "dev": "npm run serve",
    "serve": "cross-env NODE_ENV=development BUILD_EXAMPLES=true webpack serve",
    "lint": "eslint src test",
    "test": "cross-env NODE_ENV=test mocha --require @babel/register --require test/testSetup.js --colors ./test/**/*.test.js",
    "test:watch": "cross-env NODE_ENV=test mocha --require @babel/register --require test/testSetup.js --colors -w ./test/**/*.test.js",
    "prepare": "npm run build:es"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ease-live/ease-live-bridge-web.git"
  },
  "keywords": [],
  "author": "Ease Live AS",
  "publishConfig": {
    "registry": "https://easelive.jfrog.io/artifactory/api/npm/npm/"
  },
  "dependencies": {
    "es6-object-assign": "^1.1.0",
    "eventemitter2": "^6.4.5",
    "lodash": "^4.17.21",
    "ua-parser-js": "~1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "~7.17.6",
    "@babel/core": "~7.17.5",
    "@babel/preset-env": "~7.16.11",
    "@babel/register": "~7.17.0",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "~8.2.3",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-lodash": "^3.3.4",
    "chai": "^4.3.6",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-webpack-plugin": "~3.1.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jsdom": "~19.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^9.2.1",
    "process": "~0.11.10",
    "sinon": "^13.0.1",
    "terser-webpack-plugin": "~5.3.1",
    "webpack": "~5.70.0",
    "webpack-cli": "~4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-push": "npm run lint && npm run test"
    }
  }
}
